e === DO_TASK && task.role === TAILOR);\n\n  return <div className=\"App\">\n      { stage === PREGAME && <div className=\"start\">\n        <button onClick={startGame}>Start Game</button>\n        </div> }\n      <div className={`${stage === PREGAME ? \"blur\" : ''} container`}>\n<h4>Stage: {stage === DO_TASK ? roles[task.role].taskInstructions : stage} (current score: {salesScore + worksScore})</h4>\n        <div className=\"decks border\">\n          <Deck\n            deck={deck}\n            label=\"deck\" />\n          <Deck\n            deck={waitingArea}\n            label=\"Waiting area\" />\n        </div>\n      \n        <Floor \n          floor={floor} \n          doTask={highlightFloor ? taskFunction : () => {}}\n          highlight={highlightFloor} />\n\n        <div className=\"helpers-bench-and-sales border\">\n          <Helpers \n            helpers={helpers} \n            highlight={highlightHelpers} />\n          <CraftBench \n            craftBench={craftBench} \n            highlight={highlightCraftBench} \n            enableMoveToSales\n            moveToSales={enableMoveToSales ? taskFunction : () => {}}\n            />\n          <Sales \n            sales={ sales }\n            salesScore={salesScore}\n            coveredMap={coveredMap}\n            highlight={enableMoveToSales} />\n        </div>\n\n        <Works works={works} worksScore={worksScore}/>\n\n        <div className=\"task-and-hand border\">\n          <Task \n            task={ task } \n            highlight={stage === DO_TASK} \n            canCraft={canCraft}\n            craft={craftOrSmith}\n            doTask={ tailorIsClickable ? taskFunction : () => {} }\n            pray={pray} />\n          <Hand \n            placeTask={ placeTask } \n            hand={ hand }\n            taskFunction={taskFunction}\n            smithingTime={smithingTime}\n            highlight={highlightHand}\n            canSmithMap={canSmithMap}\n            pray={pray}  />\n        </div>\n\n\n        <div className=\"button-container\">\n          <button disabled={ stage !== WAIT_FOR_NEXT_TURN } onClick={startNewTurn}>\n            Draw Waiting Area\n          </button>\n            {\n              floor.length === 0 && stage !== PREGAME\n                && <button onClick={() => {\n                  changeFloor(deck.slice(0,5))\n                  changeDeck(deck.slice(6))\n                }}>\n                  Seed Floor\n                </button>\n            }      \n            </div>\n          <p> actions remaining: </p>\n          <ul>\n            { actionsQueue.map(action => <li key={Math.random()}>{action}</li>)}\n          </ul>\n          </div>\n        </div>\n     }\n\nexport default App;\n","/Users/dannycairns/mottainai/src/Card.js",[],"/Users/dannycairns/mottainai/src/roles.js",[],"/Users/dannycairns/mottainai/src/Deck.js",[],"/Users/dannycairns/mottainai/src/defaultDeck.js",[],"/Users/dannycairns/mottainai/src/Floor.js",[],"/Users/dannycairns/mottainai/src/Task.js",[],"/Users/dannycairns/mottainai/src/Hand.js",[],"/Users/dannycairns/mottainai/src/useGame.js",[],"/Users/dannycairns/mottainai/src/evaluateTask.js",[],"/Users/dannycairns/mottainai/src/utils.js",[],"/Users/dannycairns/mottainai/src/Helpers.js",[],"/Users/dannycairns/mottainai/src/CraftBench.js",[],"/Users/dannycairns/mottainai/src/Sales.js",[],"/Users/dannycairns/mottainai/src/Works.js",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":20,"column":8,"nodeType":"81","messageId":"82","endLine":20,"endColumn":23},"no-native-reassign",["83"],"no-negated-in-lhs",["84"],"no-unused-vars","'useEvaluateTask' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]